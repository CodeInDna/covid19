{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid19-pandemic-reporting-adf"
		},
		"ls_ablob_covid19_reporting_sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covid19_reporting_sa'"
		},
		"ls_adls_covid19_reporting_dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covid19_reporting_dl'"
		},
		"ls_adls_covid19_reporting_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covid19reportingdlv2.dfs.core.windows.net/"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4092e715-2d4e-4c34-a5b2-fa7431125cf0/resourceGroups/covid19-reporting-rg/providers/Microsoft.Storage/storageAccounts/covid19reportingsav2"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid19-pandemic-reporting-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covid19_reporting_sa_connectionString": {
						"value": "[parameters('ls_ablob_covid19_reporting_sa_connectionString')]"
					},
					"ls_adls_covid19_reporting_dl_accountKey": {
						"value": "[parameters('ls_adls_covid19_reporting_dl_accountKey')]"
					},
					"ls_adls_covid19_reporting_dl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covid19_reporting_dl_properties_typeProperties_url')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}